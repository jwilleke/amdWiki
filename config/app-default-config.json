{
  "amdwiki.applicationName": "amdWiki",
  "amdwiki.version": "1.3.2",
  "amdwiki.baseURL": "http://localhost:3000",
  "amdwiki.encoding": "UTF-8",
  "amdwiki.frontPage": "Welcome",
  "amdwiki.server.port": 3000,
  "amdwiki.server.host": "localhost",
  "amdwiki.session.secret": "amdwiki-session-secret-change-in-production",
  "amdwiki.session.maxAge": 86400000,
  "amdwiki.session.secure": false,
  "amdwiki.session.httpOnly": true,
  "amdwiki.templateDir": "default",
  "amdwiki.workDir": "./work",
  "amdwiki.fileSystemProvider.pageDir": "./pages",
  "amdwiki.translatorReader.matchEnglishPlurals": true,
  "amdwiki.translatorReader.camelCaseLinks": true,
  "amdwiki.pageProvider": "FileSystemProvider",
  "amdwiki.attachment.provider": "BasicAttachmentProvider",
  "amdwiki.attachment.forceDownload": false,
  "amdwiki.attachments.maxSize": 10485760,
  "amdwiki.diffProvider": "TraditionalDiffProvider",
  "amdwiki.searchProvider": "LuceneSearchProvider",
  "amdwiki.rss.generate": true,
  "amdwiki.rss.fileName": "rss.xml",
  "amdwiki.rss.interval": 3600,
  "amdwiki.rss.channelTitle": "amdWiki RSS Feed",
  "amdwiki.rss.channelDescription": "RSS feed for amdWiki updates",
  "amdwiki.breadcrumbs.maxpages": 10,
  "amdwiki.workflow.saveWikiPage": false,
  "amdwiki.workflow.approver": "Admin",
  "amdwiki.userdatabase": "JSONUserDatabase",
  "amdwiki.groupdatabase": "JSONGroupDatabase",
  "amdwiki.authorizer": "WebContainerAuthorizer",
  "amdwiki.loginModule.class": "CookieAuthenticationLoginModule",
  "amdwiki.basicAttachmentProvider.storageDir": "./attachments",
  "amdwiki.luceneSearchProvider.luceneDirectory": "./lucene",
  "amdwiki.translatorReader.allowHTML": false,
  "amdwiki.translatorReader.inlinePattern.1": "s/\\{\\{([^}]+)\\}\\}/\\<tt\\>$1\\<\\/tt\\>/g",
  "amdwiki.plugin.searchresult.showScore": true,
  "amdwiki.plugin.searchresult.maxItems": 50,
  "amdwiki.plugin.versioning.use": true,
  "amdwiki.approver.workflow.saveWikiPage": "Admin",
  "amdwiki.interWikiRef.1": "JSPWiki http://www.jspwiki.org/wiki/",
  "amdwiki.interWikiRef.2": "Doc http://doc.jspwiki.org/2.4/wiki/",
  "amdwiki.interWikiRef.3": "Wikipedia http://en.wikipedia.org/wiki/",
  "amdwiki.specialPage.Login": "Login.jsp",
  "amdwiki.specialPage.UserPreferences": "UserPreferences.jsp",
  "amdwiki.specialPage.Search": "Search.jsp",
  "amdwiki.specialPage.FindPage": "FindPage.jsp",
  "amdwiki.specialPage.Diff": "Diff.jsp",
  "amdwiki.specialPage.Workflow": "Workflow.jsp",
  "amdwiki.specialPage.Upload": "Upload.jsp",
  "amdwiki.url.generator": "DefaultURLGenerator",
  "amdwiki.referenceStyle": "relative",
  "amdwiki.pageFilters": "org.apache.wiki.filters.BasicPageFilter,org.apache.wiki.filters.ProfanityFilter",
  "amdwiki.xmlGroupDatabase.file": "./config/groupdatabase.xml",
  "amdwiki.xmlUserDatabase.file": "./config/userdatabase.xml",
  "log4j.appender.console": "org.apache.log4j.ConsoleAppender",
  "log4j.appender.console.layout": "org.apache.log4j.PatternLayout",
  "log4j.appender.console.layout.ConversionPattern": "%d [%t] %p %c %x - %m%n",
  "log4j.rootCategory": "INFO,console"
}